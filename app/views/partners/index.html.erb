<p id="notice"><%= notice %></p>

<h1>Listing Partners</h1>

<table>
  <thead>
    <tr>
      <th>Clinic</th>
      <th>Doctor</th>
      <th>Address</th>
      <th>Phone</th>
      <th>Cell</th>
      <th>Email</th>
      <th>Web</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @partners.each do |partner| %>
      <tr>
        <td><%= partner.clinic %></td>
        <td><%= partner.doctor %></td>
        <td><%= partner.address %></td>
        <td><%= partner.phone %></td>
        <td><%= partner.cell %></td>
        <td><%= partner.email %></td>
        <td><%= partner.web %></td>
        <td><%= link_to 'Show', partner %></td>
        <td><%= link_to 'Edit', edit_partner_path(partner) %></td>
        <td><%= link_to 'Destroy', partner, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Partner', new_partner_path %>
